Algoritmo negopos
	//Inicializar variable
	Definir num1 Como Real;
	//Pedir los Datos
	Imprimir "Hola bb ;), dame tu número:";
	//Leer num1
	Leer num1;
	//Condicionar si es positivo o negativo
	si num1 >= 0 Entonces
		//Imprimir pos
		Imprimir "Tu número es positivo: ", num1;
	SiNo
		//Imprimir neg
		Imprimir "Tu número es negativo: ", num1;
	FinSi
	
FinAlgoritmo
